{
    "name": "magroski/wt",
    "description": "A library to quickly encode/decode web tokens",
    "type": "library",
    "keywords": [
        "JWT",
        "WT",
        "Web Token"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Magroski",
            "email": "magroski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WT\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.10.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "4.5",
        "squizlabs/php_codesniffer": "*"
    }
}
